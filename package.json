{
  "name": "feelings.js",
  "version": "^1.0.0",
  "description": "formerly feelings.hs",
  "main": "index.js",
  "repository": "https://github.com/hlian/feelings.js",
  "author": "hao",
  "license": "MIT",
  "scripts": {
    "_prettier": "prettier \"./**/*.{js,jsx,ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit -p .",
    "test": "concurrently -n lint,tsc 'yarn jest' 'yarn typecheck'",
    "fix": "yarn _prettier --write",
    "execstart": "nodemon",
    "dev": "nodemon --watch"
  },
  "dependencies": {
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "luxon": "^1.27.0",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.9",
    "onchange": "^7.1.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.13.0",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-router": "^7.4.2",
    "@types/luxon": "^1.25.3",
    "@types/mustache": "^4.1.0",
    "@types/node": "^13.7.6",
    "@types/write-file-atomic": "^3.0.2",
    "concurrently": "^6.2.0",
    "fuse-box": "^4.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/etc/preprocessor.js"
    }
  }
}
